{% for command in item.supervisor.commands%}
[program:{{item.name}}-{{command.name}}]
process_name=worker_%(process_num)02d
directory={{item.supervisor.project_root}}
command={{command.command}}
numprocs={{command.numprocs}}
user=ubuntu
autostart=true
autorestart=true
stopasgroup=true
killasgroup=true
stopwaitsecs=3600
redirect_stderr=true
stdout_logfile={{command.stdout_logfile}}

{% endfor %}